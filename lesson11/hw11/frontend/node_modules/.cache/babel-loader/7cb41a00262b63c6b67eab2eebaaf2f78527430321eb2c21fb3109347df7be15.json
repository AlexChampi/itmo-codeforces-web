{"ast":null,"code":"import Sidebar from \"./sidebar/Sidebar\";\nimport Index from \"./main/Index\";\nimport Enter from \"./main/Enter\";\nimport Register from \"./main/Register\";\nimport Users from \"@/components/main/Users.vue\";\nimport WritePost from \"@/components/page/WritePost.vue\";\nimport ViewPost from \"@/components/page/ViewPost.vue\";\nexport default {\n  name: \"Middle\",\n  data: function () {\n    return {\n      page: \"Index\",\n      post: null\n    };\n  },\n  components: {\n    ViewPost,\n    WritePost,\n    Users,\n    Register,\n    Enter,\n    Index,\n    Sidebar\n  },\n  props: [\"posts\", \"users\", \"comments\"],\n  computed: {\n    viewPosts: function () {\n      return Object.values(this.posts).sort((a, b) => b.id - a.id).slice(0, 2);\n    }\n  },\n  beforeCreate() {\n    this.$root.$on(\"onChangePage\", page => this.page = page);\n    this.$root.$on(\"onViewPost\", post => {\n      this.post = post;\n      this.page = \"ViewPost\";\n    });\n  }\n};","map":{"version":3,"mappings":"AAeA;AACA;AACA;AACA;AACA;AACA;AACA;AAEA;EACAA;EACAC;IACA;MACAC;MACAC;IACA;EACA;EACAC;IACAC;IACAC;IACAC;IACAC;IACAC;IACAC;IACAC;EACA;EACAC;EACAC;IACAC;MACA;IACA;EACA;EAAAC;IACA;IACA;MAAA;MAAA;IAAA;EACA;AACA","names":["name","data","page","post","components","ViewPost","WritePost","Users","Register","Enter","Index","Sidebar","props","computed","viewPosts","beforeCreate"],"sourceRoot":"src/components","sources":["Middle.vue"],"sourcesContent":["<template>\r\n    <div class=\"middle\">\r\n        <Sidebar :posts=\"viewPosts\"/>\r\n        <main>\r\n            <Index v-if=\"page === 'Index'\" :posts=\"posts\" :comments=\"comments\"/>\r\n            <Users v-if=\"page === 'Users'\" :users=\"users\"/>\r\n            <WritePost v-if=\"page === 'WritePost'\"/>\r\n            <Enter v-if=\"page === 'Enter'\"/>\r\n            <Register v-if=\"page === 'Register'\"/>\r\n            <ViewPost v-if=\"page === 'ViewPost' && !post\" :post=\"post\" :comments=\"comments\"/>\r\